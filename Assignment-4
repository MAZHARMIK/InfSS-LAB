#include <stdlib.h>
#include <stdio.h>

int binaryNum[8]={0};

void decToBinary(int n)
{
	int j=0;
    	while (n > 0)
    	{
        	binaryNum[j] = n % 2;
        	n = n / 2;
        	j++;
    	}
}


